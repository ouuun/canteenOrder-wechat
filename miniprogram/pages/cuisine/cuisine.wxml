<view id="movebox">
  <block wx:for="{{typeList}}" wx:key="index">
    <!-- <view class="flex1 typeItem" id="movelist{{index}}" style="left:0;right:0;{{nowmoveindex==index?('position:absolute;top:'+movetop+'px;height:'+itemheight+'px;'):''}}"> -->
      <view class="flex1 typeItem" id="movelist{{index}}" style="left:0;right:0;{{nowmoveindex==index?('position:absolute;'):''}}">
      <view class="{{moveoutindex==index?'occupied':''}}" style="height:0;transition: 0.25s;" />
      <view class="flex1 item">
        <view class="item_name">{{item.name}}</view>
        <view capture-bind:touchend="move" bindtouchmove="move" data-index="{{index}}" style="position: absolute;right: 0.5rem;">
          <image style="width:1.5rem;height:1.5rem;" src="../../images/cuisine/drag.png"></image>
        </view>
      </view>
    </view>
  </block>
</view>